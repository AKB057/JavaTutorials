import java.util.Scanner;

public class Array1 {

	public static void main(String[] args) {
		int i,j;
		Scanner s= new Scanner(System.in);
		
		System.out.println("Enter the limit");
		int limit= s.nextInt();
		System.out.println("Enter the values");
		int ar[]= new int[limit];
		
		for(i=0;i<limit;i++) {
			ar[i]=s.nextInt();
		}
		
		for(i=0;i<limit;i++) {
			count=0;
			for(j=i+1;j<limit;j++) {
				if(ar[i]==ar[j]) {
					count++;
				}
			}
			if(count>0) {
				System.out.println(+ar[i]+" has occured"+count+"times");
			}
		}
		
	}
	
	}
